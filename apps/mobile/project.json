{
  "name": "mobile",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/mobile",
  "projectType": "application",
  "targets": {
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node_modules/.bin/react-native start"
      }
    },
    "run-ios": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node_modules/.bin/react-native run-ios"
      }
    },
    "run-android": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node_modules/.bin/react-native run-android"
      }
    },
    "build-ios": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node_modules/.bin/react-native build-ios"
      }
    },
    "build-android": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node_modules/.bin/react-native build-android"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "node_modules/.bin/jest"
      }
    }
  }
}